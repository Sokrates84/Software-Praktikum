<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension point="org.eclipse.ui.editors">
      <editor name="GEF Shapes Example"
              extensions="shapes"
              icon="shapeIcon2.png"
              default="true"
              class="com.sp.r2.editorplugin.shapes.ShapesEditor"
              contributorClass="com.sp.r2.editorplugin.shapes.ShapesEditorActionBarContributor"
              id="GEF Shapes Editor">
      </editor>
   </extension>
   <extension point="org.eclipse.ui.newWizards">
      <category name="Examples"
                parentCategory="org.eclipse.ui.Examples"
                id="com.sp.r2.editorplugin"/>
      <wizard name="Shapes Diagram"
              icon="shapeIcon2.png"
              category="org.eclipse.ui.Examples/com.sp.r2.editorplugin"
              class="com.sp.r2.editorplugin.shapes.ShapesCreationWizard"
              id="com.sp.r2.editorplugin.shapes.ShapesCreationWizard">
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.gef.examples.shapes"
            name="Sample Category">
      </category>
      <view
            category="org.eclipse.gef.examples.shapes"
            class="org.eclipse.gef.examples.shapes.views.PropertiesView"
            icon="icons/sample.gif"
            id="org.eclipse.gef.examples.shapes.views.PropertiesView"
            name="Properties View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.eclipse.gef.examples.shapes.views.PropertiesView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.sp.r2.editorplugin.handler"
               label="Open File">
            <command
                  commandId="com.sp.r2.editor-plugin.OPF_Command"
                  icon="icons/Simpsons-Folder-Small-icon.png"
                  id="OPF"
                  label="Open XML File"
                  style="push">
            </command>
         </toolbar>
         <toolbar
               id="com.sp.r2.editor-plugin.toolbar3"
               label="OP">
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.sp.r2.editorplugin.handler.OPFHandler"
            commandId="com.sp.r2.editor-plugin.OPF_Command">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.sp.r2.editor-plugin.OPF_Command"
            name="Open XML">
         <defaultHandler
               class="com.sp.r2.editorplugin.handler.OPFHandler">
            <parameter
                  name="com.sp.r2.editor-plugin.parameter3"
                  value="com.sp.r2.editor-plugin.parameter3">
            </parameter>
         </defaultHandler>
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="TestsView">
         <propertyTab
               category="first"
               id="propertyTab1"
               indented="false"
               label="Name">
         </propertyTab>
         <propertyTab
               afterTab="propertyTab1"
               category="first"
               id="propertyTab2"
               label="Project">
         </propertyTab>
         <propertyTab
               category="second"
               id="propertyTab3"
               label="Message">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="SamplePropertiesView">
         <propertyCategory></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="propertySection">
         <propertySection
               class="PropertySectionFilter"
               id="propertyTab1"
               tab="propertyTab1">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
